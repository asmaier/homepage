#box	
		{
		background-color:	#ffffff;
		min-width:	600px;
		margin-top:	-27px;
		position: 	relative; /* nötig, damit z-index funktioniert*/
		z-index:		1;
		/*width:64em;*/
		}
#bo		{
		background-image: 	url(bo.gif);
		background-repeat:	repeat-x;
		}
#blo		{
		background-image: url(blo.gif);
		background-position: top left;
      		background-repeat: no-repeat;
      		}
#bro		{
		background-image: url(bro.gif);
		background-position: top right;
      		background-repeat: no-repeat;
      		}
#bl		{
		background-image: url(bl.gif);
		background-position: left;
		background-repeat: repeat-y;
		}
#br		{
		background-image: url(br.gif);
		background-position: right;
		background-repeat: repeat-y;
		}
#bu		{
		background-image: 	url(bu.gif);
		background-position: bottom;
		background-repeat:	repeat-x;
		}		
#blu		{
		background-image: url(blu.gif);
		background-position: bottom left;
      		background-repeat: no-repeat;
      		}
#bru		{
		background-image: url(bru.gif);
		background-position: bottom right;
      		background-repeat: no-repeat;
      		}
		
#content	{
		padding-top:	32px;
		padding-bottom:	45px;
		padding-left:	80px;
		padding-right:	80px;
		}
		
div.spacer 	{
		clear: both;
		}
		
.album 		{
		margin-top: 20px;
		margin-right: -20px;
		position: 	relative; /* nötig, damit z-index funktioniert*/
		z-index:		3;
		}
		
.thumb 		{ 
		display: inline; 
		float: left; 
		margin: 4px; 
		background: transparent url(frame2.gif) no-repeat top left; 
		width: 101px; 
		height: 115px;
		position: 	relative; /* nötig, damit z-index funktioniert*/
		z-index:		3;
		}
		
.thumb a 	{ 
		display: block; 
		width: 101px; 
		height: 101px; 
		background-position: center; 
		background-repeat: no-repeat; 
		text-decoration: none; 
		}
		
.thumb a span 	{ 
		font-size: 9px; 
		position: relative; 
		top: 103px; 
		color: black;  
		text-transform: uppercase; 
		display: block; 
		text-align: center;
		width: 101px;
		overflow: hidden; 
		}
		
.thumb a:hover span { 
		color: red; 
		}		

#subpage	{
		margin-left: auto; 
		margin-right:auto;
		}
		
#subpage #center{
		display: block; 
		margin-left: auto; 
		margin-right: auto;
		}
		
#subpage #caption	{
		text-align:center; 
		margin-top: 3px; 
		text-transform: uppercase;
		}	
		
		
/* inspired by http://webdesign.about.com/od/css/a/aa022805.htm 
and http://www.webmasterworld.com/html/3426954.htm */		
blockquote p {
margin: 1em;
padding: 0px;
quotes: "\201C" "\201D";
}
blockquote p:before { content: open-quote; font-weight: bold; }
blockquote p:after { content: close-quote; font-weight: bold; }
	
blockquote .author {
margin-top:-1em;
font-style: italic;
text-align:right;
} 		
		
		
/* The W3C Core Styles */

body	{
	font-size:	14px;
	font-family:	Trebuchet ms, Verdana, Myriad Web, Syntax, sans-serif;
	/*font-family: 	Verdana, "Lucida Grande", "Lucida Sans",  "Bitstream Vera Sans", sans-serif;*/
	/*font-family:	Georgia, Minion Web, Palatino, Book Antiqua, Utopia, Times New Roman, serif;*/

	padding-top:	1em;
	border: 0;
	line-height:	1.5em;
	background-image:	url(back.gif);
	background-position:0px 4px;
	background-color:white;
	color:black;
	}
	
h1, h2, h3, h4, h5, h6	{	
	line-height:	1.33em;
	margin-bottom:	.33em;
	}	

		
h1	{
	font-size:	2em;
	font-weight:	normal;
	margin-top:	1.33em;
	
	}	
		
h2	{
	font-size:	1.75em;
	font-weight:	normal;
	margin-top:	1.75em;
	}	
		
h3	{
	font-size:	1.58em;
	font-weight:	normal;
	margin-top:	1.58em;
	}	
		
h4	{
	font-size:	1.33em;
	margin-top:	1.33em;
	}	
		
h5 {
	font-size:	1.17em;
	margin-top:	1.17em;
	
	}	
		
h6	{
	font-size:	1em;
	margin-top:	1em;
	}		

a:link	{
	text-decoration:	none;
	font-weight:	bold; 
	color:	#C00;
	}	
		
a:visited	{
	text-decoration:	none;
	font-weight:	bold;
	color:	#69C;
	}	
		
a:active	{
	text-decoration:	none;
	font-weight:	bold;
	color:	#C00;
	}	
		
a:hover	{
	text-decoration:	none;
	color:	#F00;
	}	
		
a.offsite	{
	text-decoration:	none;
	font-weight:	normal;
	color:	#C66;
	}	
	
ul, ol, dl	{	
	margin-top:	.83em;
	margin-bottom:	.83em;
	}	
	
ul ul, ol ol, li address, li dl, li ol, li p, li ul, li dir, li hr, li menu, li pre, li h1, li h2, li h3, li h4, li h5, li h6, dd address, dd dl, dd ol, dd p, dd ul, dd dir, dd hr, dd menu, dd pre, dd h1, dd h2, dd h3, dd h4, dd h5, dd h6	{	
	margin-top:	0;
	margin-bottom:	0;
	}
	
li	{	
	margin-left:	1.75em;
	}
	
img	{	
	border-style:	none;
	}	
	
pre	{
	font-size:	1em;
	font-family:	'Monotype.com', Courier New, monospace;

	margin-top:	.83em;
	margin-bottom:	.83em;
	line-height:	1.33em;
	}
	
em	{	
	font-style:	normal;
	font-weight:	bold;
	color:		black;
	}
	
td, th	{	
	line-height:	1.58em;
	}
	
address	{	
	font-style:	normal;
	letter-spacing:	.1em;

	margin-top:	1.75em;
	margin-bottom:	1.75em;
	}




























